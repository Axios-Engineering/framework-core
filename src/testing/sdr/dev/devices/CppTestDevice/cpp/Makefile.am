ossieName = CppTestDevice
bindir = $(prefix)/dev/devices/CppTestDevice/cpp/
bin_PROGRAMS = CppTestDevice

xmldir = $(prefix)/dev/devices/CppTestDevice/
dist_xml_DATA = ../CppTestDevice.scd.xml ../CppTestDevice.prf.xml ../CppTestDevice.spd.xml

distclean-local:
	rm -f config.*
	rm -rf autom4te.cache
	rm -f acinclude.m4
	rm -f aclocal.m4
	rm -f configure
	rm -f depcomp
	rm -f install-sh
	rm -f ltmain.sh
	rm -f Makefile.in
	rm -f missing
	rm -rf .deps


# Sources, libraries and library directories are auto-included from a file
# generated by the REDHAWK IDE. You can remove/modify the following lines if
# you wish to manually control these options.
include $(srcdir)/Makefile.am.ide
CppTestDevice_SOURCES = $(redhawk_SOURCES_auto)
CppTestDevice_CXXFLAGS = -Wall $(PROJECTDEPS_CFLAGS) $(BOOST_CPPFLAGS) $(INTERFACEDEPS_CFLAGS) $(redhawk_INCLUDES_auto)
CppTestDevice_LDADD = $(PROJECTDEPS_LIBS) $(BOOST_LDFLAGS) $(BOOST_THREAD_LIB) $(INTERFACEDEPS_LIBS) $(redhawk_LDADD_auto)
CppTestDevice_LDFLAGS = -Wall $(redhawk_LDFLAGS_auto)

